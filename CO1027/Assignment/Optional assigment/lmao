void process(string fileName) {
    ifstream ifs;
    ifs.open(fileName);
    
    while(ifs.get(temp)){
    
        int space = 0; int total = 0; int let = 0;
        for(int i = 0; i < n; i++){
            if((temp >= 'a' && temp <= 'z' ) || (temp >= 'A' && temp <= 'Z')) let += 1;
            else if(temp == '/0' ) space += 1;
            else total += 1;
        }   
    }
}
